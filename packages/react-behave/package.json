{
  "name": "react-behave",
  "version": "0.1.0",
  "description": "A library of React behavioral components.",
  "repository": "simonrelet/react-libraries",
  "bugs": {
    "url": "https://github.com/simonrelet/react-libraries/issues"
  },
  "keywords": [
    "react",
    "behave",
    "behaviour",
    "container",
    "component"
  ],
  "main": "build/react-behave.js",
  "module": "build/react-behave.es.js",
  "unpkg": "build/umd/react-behave.production.min.js",
  "license": "MIT",
  "files": [
    "build",
    "docs"
  ],
  "scripts": {
    "build": "react-libraries build",
    "build-storybook": "build-storybook -c storybook -o build-storybook",
    "clean": "react-libraries clean",
    "doc": "react-libraries doc",
    "start": "react-libraries build -w",
    "storybook": "start-storybook -c storybook -p 4000",
    "test": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@simonrelet/react-utils": "^0.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.5.7",
    "react-event-listener": "^0.6.0",
    "react-popper": "^1.0.0-beta.6"
  },
  "devDependencies": {
    "@simonrelet/react-libraries": "^0.1.0",
    "@storybook/addon-a11y": "^3.4.5",
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-centered": "^3.4.5",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-knobs": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/addon-options": "^3.4.5",
    "@storybook/addon-viewport": "^3.4.6",
    "@storybook/addons": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.3.2"
  }
