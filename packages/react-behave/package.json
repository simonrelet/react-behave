{
  "name": "react-behave",
  "version": "0.6.0",
  "description": "A library of React behavioral components.",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrelet/react-libraries"
  },
  "bugs": {
    "url": "https://github.com/simonrelet/react-libraries/issues"
  },
  "keywords": [
    "react",
    "behave",
    "behaviour",
    "container",
    "component"
  ],
  "main": "build/cjs",
  "module": "build/es",
  "license": "MIT",
  "files": [
    "build",
    "docs"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "react-libraries build",
    "build:storybook": "build-storybook -o build-storybook",
    "bump-version": "react-libraries bump-version",
    "clean": "react-libraries clean",
    "doc": "react-libraries doc",
    "prebump-version": "npm-run-all clean build doc",
    "start": "react-libraries build -w",
    "storybook": "start-storybook -p 4000",
    "test": "react-libraries test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "memoize-one": "^4.0.0",
    "prop-types": "^15.5.7",
    "react-popper": "^1.0.0-beta.6",
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@simonrelet/react-libraries": "^0.15.0",
    "@storybook/addon-a11y": "^4.0.4",
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-centered": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-links": "^4.0.4",
    "@storybook/addon-options": "^4.0.4",
    "@storybook/addon-viewport": "^4.0.4",
    "@storybook/addons": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "npm-run-all": "^4.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.3.2"
  }
}
