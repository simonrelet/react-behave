{
  "name": "react-behave",
  "version": "0.5.0",
  "description": "A library of React behavioral components.",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrelet/react-libraries"
  },
  "bugs": {
    "url": "https://github.com/simonrelet/react-libraries/issues"
  },
  "keywords": [
    "react",
    "behave",
    "behaviour",
    "container",
    "component"
  ],
  "main": "build/react-behave.js",
  "module": "build/react-behave.es.js",
  "license": "MIT",
  "files": [
    "build",
    "docs"
  ],
  "scripts": {
    "build": "react-libraries build",
    "build-storybook": "build-storybook -o build-storybook",
    "bump-version": "react-libraries bump-version",
    "clean": "react-libraries clean",
    "doc": "react-libraries doc",
    "prebump-version": "npm-run-all clean build doc",
    "start": "react-libraries build -w",
    "storybook": "start-storybook -p 4000",
    "test": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "dom-scroll-into-view": "^1.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "memoize-one": "^4.0.0",
    "prop-types": "^15.5.7",
    "react-event-listener": "^0.6.0",
    "react-popper": "^1.0.0-beta.6",
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@simonrelet/react-libraries": "^0.12.0",
    "@storybook/addon-a11y": "^4.0.0-alpha.9",
    "@storybook/addon-actions": "^4.0.0-alpha.9",
    "@storybook/addon-centered": "^4.0.0-alpha.9",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-knobs": "^4.0.0-alpha.9",
    "@storybook/addon-links": "^4.0.0-alpha.9",
    "@storybook/addon-options": "^4.0.0-alpha.9",
    "@storybook/addon-viewport": "^4.0.0-alpha.9",
    "@storybook/addons": "^4.0.0-alpha.9",
    "@storybook/react": "^4.0.0-alpha.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "npm-run-all": "^4.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/**/index.js",
      "!src/ReactSixteenAdapter.js"
    ]
  }
}
