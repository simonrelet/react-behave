{
  "name": "@simonrelet/react-ui",
  "version": "0.1.0",
  "description": "A library of React UI components.",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrelet/react-libraries"
  },
  "bugs": {
    "url": "https://github.com/simonrelet/react-libraries/issues"
  },
  "keywords": [
    "react",
    "ui",
    "component"
  ],
  "main": "build/react-ui.js",
  "module": "build/react-ui.es.js",
  "license": "MIT",
  "files": [
    "build",
    "docs"
  ],
  "scripts": {
    "build": "react-libraries build",
    "build-storybook": "build-storybook -c storybook -o build-storybook",
    "bump-version": "react-libraries bump-version",
    "clean": "react-libraries clean",
    "doc": "react-libraries doc",
    "start": "react-libraries build -w",
    "storybook": "start-storybook -c storybook -p 4000",
    "test": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "change-case": "^3.0.2",
    "classnames": "^2.2.5",
    "lodash.mergewith": "^4.6.1",
    "polished": "^1.9.2",
    "prop-types": "^15.5.7",
    "react-behave": "^0.3.0",
    "styled-components": "^3.3.2"
  },
  "devDependencies": {
    "@simonrelet/react-libraries": "^0.2.0",
    "@storybook/addon-a11y": "^4.0.0-alpha.9",
    "@storybook/addon-actions": "^4.0.0-alpha.9",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-knobs": "^4.0.0-alpha.9",
    "@storybook/addon-links": "^4.0.0-alpha.9",
    "@storybook/addon-options": "^4.0.0-alpha.9",
    "@storybook/addon-viewport": "^4.0.0-alpha.9",
    "@storybook/addons": "^4.0.0-alpha.9",
    "@storybook/react": "^4.0.0-alpha.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/**/index.js",
      "!src/ReactSixteenAdapter.js"
    ]
  }
}
