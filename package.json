{
  "name": "react-behave",
  "version": "1.0.0-alpha.5",
  "description": "A library of behaviors for React applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrelet/react-behave"
  },
  "bugs": {
    "url": "https://github.com/simonrelet/react-behave/issues"
  },
  "keywords": [
    "react",
    "behave",
    "behaviour",
    "container",
    "component"
  ],
  "main": "build/cjs",
  "module": "build/es",
  "license": "MIT",
  "files": [
    "build",
    "docs"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "react-libraries build -c \"src/**/*.ts\"",
    "build:clean": "run-s clean build",
    "build:storybook": "build-storybook -o build-storybook",
    "bump-version": "react-libraries bump-version",
    "clean": "react-libraries clean",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "lint": "run-s lint:*",
    "lint:format": "prettier --list-different \"**/*.{js,ts,json,md}\"",
    "lint:js": "eslint .",
    "prebump-version": "run-s clean build doc",
    "readme": "react-libraries readme",
    "start": "run-s \"build -w\"",
    "start:storybook": "start-storybook -p 4000 --ci",
    "test": "react-libraries test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "focus-trap": "^4.0.2",
    "invariant": "^2.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "memoize-one": "^5.0.0",
    "prop-types": "^15.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "warning": "^4.0.2"
  },
  "devDependencies": {
    "@reactions/component": "^2.0.2",
    "@simonrelet/react-libraries": "^1.0.0-alpha.4",
    "@storybook/addon-a11y": "^4.1.11",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-centered": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-options": "^4.1.11",
    "@storybook/addon-viewport": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-popper": "^1.3.3",
    "react-testing-library": "^5.4.4"
  }
}
