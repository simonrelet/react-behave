{
  "name": "react-behave",
  "version": "1.0.0-alpha.8",
  "description": "A library of behaviors for React applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrelet/react-behave"
  },
  "bugs": {
    "url": "https://github.com/simonrelet/react-behave/issues"
  },
  "keywords": [
    "react",
    "behave",
    "behaviour",
    "container",
    "component"
  ],
  "main": "build/cjs",
  "module": "build/es",
  "license": "MIT",
  "files": [
    "build",
    "docs"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "react-libraries build -c \"src/**/*.ts\"",
    "build:clean": "run-s clean build",
    "build:storybook": "build-storybook -o build-storybook",
    "bump-version": "react-libraries bump-version",
    "clean": "react-libraries clean",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "lint": "run-s lint:*",
    "lint:format": "prettier --list-different \"**/*.{js,ts,json,md}\"",
    "lint:js": "eslint .",
    "prebump-version": "run-s clean build",
    "readme": "react-libraries readme",
    "start": "run-s \"build -w\"",
    "start:storybook": "start-storybook -p 4000 --ci",
    "test": "react-libraries test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "popper.js": "^1.15.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@reactions/component": "^2.0.2",
    "@simonrelet/react-libraries": "^1.0.0",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-centered": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@storybook/theming": "^5.1.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-testing-library": "^7.0.0"
  }
}
